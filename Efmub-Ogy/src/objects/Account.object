    <validationRules>
        <fullName>Customer_Channel_Required</fullName>
        <active>true</active>
        <description>Either a Phone number, email address or mailing address is required</description>
        <errorConditionFormula>AND( 
OR( 
ISBLANK( PersonMailingStreet ), 
ISBLANK( PersonMailingCity ), 
ISBLANK( PersonMailingState ), 
ISBLANK( PersonMailingPostalCode ),
ISBLANK( PersonMailingCountry )

), 
ISBLANK( PersonEmail ), 
ISBLANK( Phone ),
NOT(Flight_Crew_from_Apex__c) 
)</errorConditionFormula>
        <errorMessage>Either a Phone number, email address or a complete mailing address is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Name_Required</fullName>
        <active>true</active>
        <description>First Name is required when creating a new Person Account</description>
        <errorConditionFormula>ISBLANK( FirstName )</errorConditionFormula>
        <errorMessage>First Name is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FlyingBlue_Number_Length_Validation</fullName>
        <active>true</active>
        <description>Allows only 10 characters</description>
        <errorConditionFormula>not( LEN( Flying_Blue_Number__pc ) = 10) &amp;&amp; Flying_Blue_Number__pc !=&apos;&apos;</errorConditionFormula>
        <errorDisplayField>Flying_Blue_Number__pc</errorDisplayField>
        <errorMessage>FlyingBlue# should be 10 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_email_address</fullName>
        <active>false</active>
        <description>Validation rule to check if the agent submitted the valid format of email address</description>
        <errorConditionFormula>IF( AND( NOT(REGEX( PersonEmail,&apos;([a-zA-Z0-9_\\-\\.]+)@((\\[a-z]{1,3}\\.[a-z]{1,3}\\.[a-z]{1,3}\\.)|(([a-zA-Z0-9\\-]+\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})&apos;)), NOT(REGEX( Work_Email__pc ,&apos;([a-zA-Z0-9_\\-\\.]+)@((\\[a-z]{1,3}\\.[a-z]{1,3}\\.[a-z]{1,3}\\.)|(([a-zA-Z0-9\\-]+\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})&apos;)), NOT(REGEX( Other_Email__pc ,&apos;([a-zA-Z0-9_\\-\\.]+)@((\\[a-z]{1,3}\\.[a-z]{1,3}\\.[a-z]{1,3}\\.)|(([a-zA-Z0-9\\-]+\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})&apos;)) ), true, false )</errorConditionFormula>
        <errorMessage>Invalid Email address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Should_be_Numeric</fullName>
        <active>false</active>
        <description>To verify that the phone number field is not a free form field</description>
        <errorConditionFormula>OR(!REGEX(PersonMobilePhone, &quot;\\+{0,1}\\({0,1}[0-9]*\\){0,1}\\s{0,1}[0-9]*-{0,1}[0-9]*&quot;),
!REGEX(PersonOtherPhone, &quot;\\+{0,1}\\({0,1}[0-9]*\\){0,1}\\s{0,1}[0-9]*-{0,1}[0-9]*&quot;),
!REGEX(Phone, &quot;\\+{0,1}\\({0,1}[0-9]*\\){0,1}\\s{0,1}[0-9]*-{0,1}[0-9]*&quot;))</errorConditionFormula>
        <errorMessage>Phone number cannot be alphanumeric</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Skymiles_Number_Length_Validation</fullName>
        <active>true</active>
        <description>Allows only 10 characters</description>
        <errorConditionFormula>not( LEN(SkyMiles_Number__pc) = 10) &amp;&amp; SkyMiles_Number__pc !=&apos;&apos;</errorConditionFormula>
        <errorDisplayField>SkyMiles_Number__pc</errorDisplayField>
        <errorMessage>Skymiles# should be 10 digits</errorMessage>
    </validationRules>
